{
  "manifest_version": 2,

  "name": "Trump-vision",
  "description": "This extension will replace all trump photos with cat photos",
  "version": "1.0.4",

  "browser_action": {
   "default_icon": "info.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://localhost:3001",
    "*://aqueous-sands-87859.herokuapp.com/*"
  ],
 "content_scripts": [
   {
     "matches": [
       "<all_urls>"
     ],
     "js": ["content.js"]
   }
 ]
}
