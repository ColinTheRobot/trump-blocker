{
  "manifest_version": 2,

  "name": "Trump-Blocker",
  "description": "This extension replaces Trumps face with cats",
  "version": "1.0.5",

  "browser_action": {
   "default_icon": "info.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://localhost:3001",
    "*://aqueous-sands-87859.herokuapp.com/*"
  ],
 "content_scripts": [
   {
     "matches": [
       "<all_urls>"
     ],
     "exclude_matches": [
       "*://google.com/*",
       "*://twitter.com/*",
       "*://facebook.com/*"
     ],
     "js": ["content.js"]
   }
 ]
}
